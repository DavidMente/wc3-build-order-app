{
  "extends": "tslint:recommended",
  "rules": {
    "member-ordering": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": false,
    "interface-name": [
      true,
      "never-prefix"
    ]
  },
  "linterOptions": {
    "exclude": [
      "./node_modules/**"
    ]
  }
}
